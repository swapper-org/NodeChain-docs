                        }
                    }
                },
                "required": [
                    "txHashes"
                ]
            },
            "GetTransactionsResponseModel": {
                "type": "object",
                "properties": {
                    "transactions": {
                        "type": "array",
                        "items": {
                            "type": "object",
                            "properties": {
                                "txHash": {
                                    "$ref": "./model-components.json#/components/schemas/TransactionHashModel"
                                },
                                "fee": {
                                    "description": "Transaction fee",
