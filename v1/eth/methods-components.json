                    "pageSize": {
                        "description": "Number of transacionts to return per page",
                        "type": "integer",
                        "maximum": 50,
                        "minimum": 0,
                        "default": 0,
                        "example": 25
                    },
                    "order": {
                        "description": "Criteria to sort the transactions in the page returned. It accepts two values, 'desc' to return the most recent transactions first, and 'asc' to return the oldest transactions first",
                        "type": "string",
                        "default": "desc",
                        "example": "desc"
                    }
                },
                "required": [
                    "addresses"
                ]
