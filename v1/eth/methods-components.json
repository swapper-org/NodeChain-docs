                        "$ref": "./model-components.json#/components/schemas/TransactionHashModel"
                    }
                },
                "required": [
                    "txHash"
                ]
            },
            "GetTransactionResponseModel": {
                "type": "object",
                "properties": {
                    "transaction": {
                        "type": "object",
                        "properties": {
                            "txHash": {
                                "$ref": "./model-components.json#/components/schemas/TransactionHashModel"
                            },
                            "fee": {
                                "description": "Transaction fee",
                                "type": "string",
                                "example": "7982291"
                            },
                            "blockHash": {
                                "description": "Ethereum block hash",
                                "type": "string",
                                "example": "0x6403b287ab8bd587cd9e07c3d2ca62269f19dfb7c195892980f14d71fefa7cdd"
                            },
                            "blockNumber": {
                                "oneOf": [
                                    {
                                        "description": "Ethereum block height",
                                        "type": "string",
                                        "example": "1"
                                    },
                                    {
                                        "type": "object",
                                        "value": null
                                    }
                                ]
                            },
                            "timestamp": {
                                "oneOf": [
                                    {
                                        "description": "Timestamp in which transaction was confirmed. Null if transaction is unconfirmed",
                                        "type": "string",
                                        "example": "1653156295"
                                    },
                                    {
                                        "type": "object",
                                        "value": null
                                    }
                                ]
                            },
                            "inputs": {
                                "type": "array",
                                "items": {
                                    "type": "object",
                                    "properties": {
                                        "address": {
                                            "description": "Ethereum address",
                                            "type": "string",
                                            "example": "0x625ACaEdeF812d2842eFd2Fb0294682A868455bd"
                                        },
                                        "amount": {
                                            "description": "Value in WEI",
                                            "type": "string",
                                            "example": "1000000000000000000"
                                        }
                                    }
                                }
                            },
                            "outputs": {
                                "type": "array",
                                "items": {
                                    "type": "object",
                                    "properties": {
                                        "address": {
                                            "description": "Ethereum address",
                                            "type": "string",
                                            "example": "0x625ACaEdeF812d2842eFd2Fb0294682A868455bd"
                                        },
                                        "amount": {
                                            "description": "Value in WEI",
                                            "type": "string",
                                            "example": "1000000000000000000"
                                        }
                                    }
                                }
                            },
                            "data": {
                                "$ref": "./model-components.json#/components/schemas/TransactionModel"
                            }
                        }
                    }
                }
            },
            "GetTransactionsRequestModel": {
                "type": "object",
