                                "outputs": {
                                    "type": "array",
                                    "items": {
                                        "type": "object",
                                        "properties": {
                                            "address": {
                                                "description": "Ethereum address",
                                                "type": "string",
                                                "example": "0x625ACaEdeF812d2842eFd2Fb0294682A868455bd"
                                            },
                                            "amount": {
                                                "description": "Value in WEI",
                                                "type": "string",
                                                "example": "1000000000000000000"
                                            }
                                        }
                                    }
                                },
                                "data": {
                                    "$ref": "./model-components.json#/components/schemas/TransactionModel"
                                }
                            }
                        }
                    }
                }
            },
            "CallRequestModel": {
                "type": "object",
                "properties": {
                    "transaction": {
                        "$ref": "./model-components.json#/components/schemas/TransactionCallModel"
                    },
                    "blockNumber": {
                        "oneOf": [
                            {
                                "type": "string",
                                "enum": [
                                    "latest",
                                    "earliest",
                                    "pending"
                                ],
                                "example": "latest"
                            },
                            {
                                "type": "string",
                                "pattern": "[0-9]"
                            }
                        ]
                    }
                },
                "required": [
                    "transaction",
                    "blockNumber"
                ]
            },
            "CallResponseModel": {
                "type": "object",
                "properties": {
                    "data": {
                        "description": "The return value of executed contract",
                        "type": "string",
                        "example": "0xd46e8dd67c5d32be8d46e8dd67c5d32be8058bb8eb970870f072445675058bb8eb970870f072445675"
                    }
                }
            },
            "GetAddressHistoryRequestModel": {
                "type": "object",
                "properties": {
                    "address": {
                        "description": "Ethereum address",
                        "type": "string",
                        "example": "0x625ACaEdeF812d2842eFd2Fb0294682A868455bd"
                    },
                    "page": {
                        "description": "Number of page to return",
                        "type": "integer",
                        "minimum": 0,
                        "default": 0,
                        "example": 0
                    },
                    "pageSize": {
                        "description": "Number of transacionts to return per page",
                        "type": "integer",
                        "maximum": 50,
                        "minimum": 0,
                        "default": 0,
                        "example": 25
                    },
                    "order": {
                        "description": "Criteria to sort the transactions in the page returned. It accepts two values, 'desc' to return the most recent transactions first, and 'asc' to return the oldest transactions first",
                        "type": "string",
                        "default": "desc",
                        "example": "desc"
                    }
                },
                "required": [
                    "address"
                ]
            },
            "GetAddressHistoryResponseModel": {
                "type": "object",
                "properties": {
                    "address": {
                        "description": "Ethereum address",
                        "type": "string",
                        "example": "0x625ACaEdeF812d2842eFd2Fb0294682A868455bd"
                    },
                    "txHashes": {
                        "type": "array",
                        "items": {
                            "$ref": "./model-components.json#/components/schemas/TransactionHashModel"
                        }
                    },
                    "maxPage": {
                        "description": "Max page to return with the parameters 'page' and 'pageSize' set in the request",
                        "type": "integer",
                        "example": 12
                    }
                },
                "required": [
                    "address",
                    "txHahes",
                    "maxPage"
                ]
            },
