            "GetAddressesHistoryResponseModel": {
                "type": "array",
                "items": {
                    "type": "object",
                    "properties": {
                        "address": {
                            "description": "Ethereum address",
                            "type": "string",
                            "example": "0x625ACaEdeF812d2842eFd2Fb0294682A868455bd"
                        },
                        "txHashes": {
                            "type": "array",
                            "items": {
                                "$ref": "./model-components.json#/components/schemas/TransactionHashModel"
                            }
                        },
                        "maxPage": {
                            "description": "Max page to return with the parameters 'page' and 'pageSize' set in the request",
                            "type": "integer",
                            "example": 12
                        }
                    },
                    "required": [
                        "address",
                        "txHahes",
                        "maxPage"
                    ]
                }
            },
