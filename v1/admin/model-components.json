{
    "components": {
        "schemas": {
            "BtcConfigurationModel": {
                "description": "Bitcoin package configuration properties",
                "type": "object",
                "properties": {
                    "bitcoincoreRpcEndpoint": {
                        "description": "Bitcoincore RPC endpoint",
                        "type": "string",
                        "example": "http://swapper:swapper@bitcoincore-regtest:8332"
                    },
                    "electrumRpcEndpoint": {
                        "description": "Electrum RPC node endpoint",
                        "type": "string",
                        "example": "http://swapper:swapper@electrum-regtest:30000"
                    },
                    "bitcoincoreZmqEndpoint": {
                        "description": "Bitcoincore node ZMQ endpoint",
                        "type": "string",
                        "example": "tcp://bitcoincore-regtest:28332"
                    },
                    "bitcoinAddressCallbackHost": {
                        "description": "Callback endpoint for addresses balance changes to be called from electrum",
                        "type": "string",
                        "example": "http://connector:80"
                    },
                    "electrsEndpoint": {
                        "description": "Electrs endpoint",
                        "type": "string",
                        "example": "electrs-regtest:60001:t"
                    }
                }
            },
            "EthConfigurationModel": {
                "description": "Ethereum package configuration properties",
                "type": "object",
                "properties": {
                    "rpcEndpoint": {
                        "description": "Ethereum node RPC endpoint",
                        "type": "string",
                        "example": "http://ethereumgo-regtest:8545"
                    },
                    "wsEndpoint": {
                        "description": "Ethereum node WS endpoint",
                        "type": "string",
                        "example": "ws://ethereumgo-regtest:8546"
                    },
                    "indexerEndpoint": {
                        "description": "Indexer node endpoint",
                        "type": "string",
                        "example": "http://postgrest-regtest:3000"
                    }
                }
            },
            "BchConfigurationModel": {
                "description": "Bitcoin cash package configuration properties",
                "type": "object",
                "properties": {
                    "bitcoincoreRpcEndpoint": {
                        "description": "Bitcoincore RPC endpoint",
                        "type": "string",
                        "example": "http://swapper:swapper@bitcoinabc-regtest:8332"
                    },
                    "electrumRpcEndpoint": {
                        "description": "Electrum cash RPC node endpoint",
                        "type": "string",
                        "example": "http://swapper:swapper@electrum-regtest:30000"
                    }
                }
            },
            "LunaConfigurationModel": {
                "description": "Luna package configuration properties",
                "type": "object",
                "properties": {
                    "terradRpcEndpoint": {
                        "description": "Terra node RPC endpoint",
                        "type": "string",
                        "example": "http://terrad-testnet:1317"
                    }
                }
            }
        }
    }
}